{
  "title": "Golden Signals (FastAPI)",
  "timezone": "",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Traffic (RPS)",
      "targets": [{"expr":"sum(rate(http_requests_total[1m]))","refId":"A"}],
      "gridPos": {"h":8,"w":12,"x":0,"y":0}
    },
    {
      "type": "timeseries",
      "title": "Error rate (req/s)",
      "targets": [{"expr":"sum(rate(http_request_errors_total[1m]))","refId":"A"}],
      "gridPos": {"h":8,"w":12,"x":12,"y":0}
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "targets": [{"expr":"histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))","refId":"A"}],
      "gridPos": {"h":8,"w":12,"x":0,"y":8}
    },
    {
      "type": "timeseries",
      "title": "Saturation (in-progress)",
      "targets": [{"expr":"sum(inprogress_requests)","refId":"A"}],
      "gridPos": {"h":8,"w":12,"x":12,"y":8}
    }
  ]
}
